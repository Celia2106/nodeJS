extends layout

block content
    .main.container
        .row
            .col.s12.offset-m3.m5.offset-l3.l5.offset-xl3.xl5.formUser
                h3.display-4.m-b-2 Borrado Usuarios
                    form(method='POST' action='/deleteuser')
                        div.form-group
                            label(for='users') Usuarios:
                            select#users.form-control(name='users')
                                each variable in variables
                                    option(value=variable.user) #{variable.user}
                            block content
                            button.btn.btn-primary.register(type='submit') Borrar Usuario